{
  "name": "inpsyde/wonolog",
  "description": "Monolog-based logger package for WordPress",
  "keywords": [
    "wordpress",
    "logger",
    "logging",
    "monolog",
    "psr-3",
    "debug",
    "debugging"
  ],
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Inpsyde GmbH",
      "email": "hello@inpsyde.com",
      "homepage": "http://inpsyde.com",
      "role": "Company"
    },
    {
      "name": "Giuseppe Mazzapica",
      "email": "giuseppe.mazzapica@gmail.com",
      "role": "Developer"
    }
  ],
  "minimum-stability": "stable",
  "require": {
    "php": ">=5.5",
    "monolog/monolog": "@stable"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.8.9",
    "brain/monkey": "~1.4.2",
    "mikey179/vfsStream": "~1.6.0"
  },
  "autoload": {
    "psr-4": {
      "Inpsyde\\Wonolog\\": "src/"
    },
    "files": [
      "inc/bootstrap.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Inpsyde\\Wonolog\\Tests\\": "tests/src/"
    }
  },
  "config": {
    "optimize-autoloader": true
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  }
}
